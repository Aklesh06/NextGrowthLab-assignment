{
    "version": 2,
    "builds": [
      {
        "src": "problem2/wsgi.py",
        "use": "@vercel/python",
        "config": {"runtime": "python3.11","buildCommand": "bash build_files.sh" }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "problem2/wsgi.py"
      }
    ]
  }